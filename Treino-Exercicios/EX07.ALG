algoritmo "semnome"
var
   n1, ter, dobro: real
inicio
escreva("digite um numero: ")
leia(n1)

dobro <- n1*2
ter <- n1 / 3

escreval("===================")
escreval("    RESULTADO      ")
escreval("O dobro de ",n1," é: ",dobro)
escreval("A terça parte de ",n1," é: ",ter )
escreval("===================")


fimalgoritmo