algoritmo "EX04"
var
   v1,v2, resultado: inteiro
inicio
      escreva("Digite um valor: ")
      leia(v1)
      escreva("Digite outro valor: ")
      leia(v2)
      
      resultado <- v1 + v2
      
      escreva("A soma de ",v1," + ",v2," é igual a: ",resultado )
      
      
fimalgoritmo