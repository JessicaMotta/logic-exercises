algoritmo "semnome"
var
   n1,n2, media: real
inicio
   escreva("Matemática - Digite a sua primeira nota: ")
   leia(n1)
   escreva("Matemática - Digite a sua segunda nota: ")
   leia(n2)
   
   media <- (n1+n2)/2
   
   escreval("Resultado ....")
   escreva("A média entre ",n1," e ",n2," é igual a ",media)
   
fimalgoritmo